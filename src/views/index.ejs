<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- favicon -->
        <link rel="icon"
              href="https://s3.amazonaws.com/www.miniroom.online/images/favicon.ico"
              type="image/x-icon">
        <!-- Vendor CSS Files -->
        <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
              rel="stylesheet">
        <link href="https://getbootstrap.com/docs/5.3/assets/css/docs.css"
              rel="stylesheet">
        <!-- Template Main CSS File -->
        <link rel="stylesheet" href="/css/index.css">
        <title>Mini Room</title>
    </head>
    <body>
        <!-- ======= Header ======= -->
        <header class="center">
            <nav class="navbar navbar-grid">
                <div>
                    <a class="navbar-brand align-center" href="/">
                        <img src="https://s3.amazonaws.com/www.miniroom.online/images/logo.png"
                             alt=""
                             class="d-inline-block align-text-top">
                        <span class="logo">Mini Room</span>
                    </a>
                </div>
                <!-- <div class="setting-container">
                    <img src="https://s3.amazonaws.com/www.miniroom.online/images/settings.png" alt=""/>
                </div>
                <div class="avatar-container">
                    <img src="https://s3.amazonaws.com/www.miniroom.online/images/avatar.png" alt=""/>
                </div> -->
            </nav>
            <!-- .logo -->
        </header>
        <!-- End Header -->
        <!-- ======= Hero Section ======= -->
        <section class="hero center">
            <div class="container">
                <!-- ======= row ======= -->
                <div class="row align-center">
                    <div class="col-lg-6 left-contet" data-aos="fade-up" data-aos-delay="200">
                        <h1>Ready For Your Meeting from any device</h1>
                        <h2>We provide a room for your video confernce</h2>
                        <div class="align-center">
                            <div class="input-group">
                                <!-- dropdown-button -->
                                <button class="btn btn-outline-secondary dropdown-toggle drop-down-btn align-center"
                                        type="button"
                                        data-bs-toggle="dropdown"
                                        aria-expanded="false">
                                    <img src="https://s3.amazonaws.com/www.miniroom.online/images/video-plus.png"
                                         alt=""
                                         class="video-plus"/>
                                    <span>New meeting</span>
                                </button>
                                <ul class="dropdown-menu">
                                    <!-- <li>
                                        <a class="dropdown-item" href=""><i class="fa-solid fa-link"></i>Create a meeting for later</a>
                                    </li> -->
                                    <li id="startBtn">
                                        <a class="dropdown-item"><i class="fa-solid fa-plus"></i>Start an instant meeting</a>
                                    </li>
                                    <!-- <li>
                                        <a class="dropdown-item" href=""><i class="fa-regular fa-calendar"></i>Schedule in Google Calendar</a>
                                    </li> -->
                                </ul>
                                <!-- input -->
                                <input type="text"
                                       class="form-control"
                                       aria-label="Text input with dropdown button"
                                       placeholder="Enter a code or link">
                            </div>
                            <button disabled id="joinBtn" class="btn join-btn disable none" type="btn">Join</button>
                        </div>
                    </div>
                    <div id="alert"
                         class="alert alert-warning d-flex align-items-center alert-container none"
                         role="alert">
                        <svg xmlns="http://www.w3.org/2000/svg"
                             class="bi bi-exclamation-triangle-fill flex-shrink-0 me-2"
                             viewBox="0 0 16 16"
                             role="img"
                             aria-label="Warning:">
                            <path d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z"/>
                        </svg>
                        <div>The link or code may be invalid, please try again!</div>
                    </div>
                    <div class="col-lg-6 right-contet order-1 order-lg-2 hero-img aos-init aos-animate center"
                         data-aos="zoom-in"
                         data-aos-delay="200">
                        <img src="https://s3.amazonaws.com/www.miniroom.online/images/hero-img.png"
                             class="img-fluid animated"
                             alt="">
                    </div>
                </div>
                <!-- /.row -->
            </div>
        </section>
        <!-- End Hero -->
    </body>
</html>
<!-- Vendor JS Files -->
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
<script src="https://kit.fontawesome.com/8de498932c.js" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
<!-- Template Main JS File -->
<script type="module" src="/js/index/index.js"></script>
